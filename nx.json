{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "installation": {
    "version": "21.4.1"
  },
  "defaultProject": "api",
  "projects": {
    "api": "apps/api",
    "web": "apps/web"
  },
  "targetDefaults": {
    "build": {
      "cache": true
    },
    "lint": {
      "cache": true
    }
  },
  "generators": {
    "@nx/node:application": {
      "directory": "apps"
    },
    "@nx/react:application": {
      "directory": "apps"
    }
  }
}
