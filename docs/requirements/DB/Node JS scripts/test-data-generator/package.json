{
  "name": "forward-test-data-generator",
  "version": "1.0.0",
  "description": "Generate realistic test data for Forward Inheritance platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "generate": "npm run build && npm start",
    "clean": "npm run build && npm start -- --clean",
    "forward-scenario": "npm run build && node dist/forward-scenario.js",
    "forward-generate": "npm run build && node dist/forward-scenario.js generate",
    "forward-clean": "npm run build && node dist/forward-scenario.js clean --confirm",
    "forward-info": "npm run build && node dist/forward-scenario.js info",
    "test-procedures": "npm run build && node dist/test-stored-procedures.js",
    "test-procs": "tsx src/test-stored-procedures.ts",
    "test-with-output": "npm run build && node dist/test-stored-procedures-with-output.js",
    "test-output": "tsx src/test-stored-procedures-with-output.ts",
    "test-comprehensive": "npm run build && node dist/run-comprehensive-tests.js",
    "test-comp": "tsx src/run-comprehensive-tests.ts",
    "test-safe": "npm run build && node dist/run-safe-tests.js",
    "test-safe-quick": "tsx src/run-safe-tests.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.1",
    "ora": "^5.4.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
