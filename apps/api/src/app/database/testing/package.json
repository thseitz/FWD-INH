{
  "name": "forward-test-data-generator",
  "version": "1.0.0",
  "description": "Generate realistic test data for Forward Inheritance platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test:1:populate": "tsx 1-populate-database.ts",
    "test:2:procedures": "tsx 2-test-stored-procedures.ts",
    "test:3:sql-files": "tsx 3-test-sql-files.ts",
    "test:suite": "tsx run-all-tests.ts",
    "test:all": "npm run test:1:populate && npm run test:2:procedures && npm run test:3:sql-files",
    "test": "npm run test:suite",
    "populate": "tsx 1-populate-database.ts",
    "test:old": "tsx test-all-procedures.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.1",
    "ora": "^5.4.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
